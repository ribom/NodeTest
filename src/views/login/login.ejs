<% include ../master/siteHeader %>

    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1>Log in to see more!</h1>
                <form action="/authenticate/login" method="post">
                    <div class="col-lg-12 padding-10">
                        <div class="col-lg-2 t-r">Name:</div>
                        <div class="col-lg-5">
                            <input name="userName" id="userName" type="text" />
                        </div>
                    </div>
                    <div class="col-lg-12 padding-10">
                        <div class="col-lg-2 t-r">Password:</div>
                        <div class="col-lg-5">
                            <input name="password" id="password" type="password" />
                        </div>
                    </div>
                    <div class="col-lg-12 padding-10">
                        <div class="col-lg-2 t-r"></div>
                        <div class="col-lg-5">
                            <button id="submit" type="submit">Log in</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <% include ../master/siteFooter %>
